{
	"name": "vesparny/silex-simple-rest",
	"type": "library",
	"description": "A simple silex skeleton for rest api",
	"keywords": [
		"framework", "silex", "rest", "api"
	],
	"homepage": "http://github.com/vesparny/silex-simple-rest",
	"license": "MIT",
	"authors": [{
		"name": "Alessandro Arnodo",
		"email": "alessandro@arnodo.net"
	}],
	"require": {
		"php": ">=5.5.12",
		"silex/silex": "~1.1",
		"monolog/monolog": "~1.6",
		"doctrine/dbal": "~2.4",
		"nesbot/Carbon": "~1.6",
		"facebook/graph-sdk": "~5.0",
		"predis/predis": "1.1.*@dev",
		"predis/service-provider": "~1.0",
		"psr/log": "~1.0",
		"symfony/http-foundation": "~2.8",
		"symfony/http-kernel": "~2.8",
		"symfony/security": "~2.8",
		"symfony/validator": "~2.8",
		"guzzlehttp/guzzle": "~6.0",
		"lib-openssl": "*",
		"google/apiclient": "^2.0.0@RC"

	},
	"require-dev": {
		"phpunit/phpunit": "~4.1.4",
		"mockery/mockery": "~0.9.1"
	},
	"autoload": {
		"psr-0": {
			"App": "src/"
		},
		"psr-4": {
			"App\\": "src",
			"App\\Validators\\": "src/App/Validator/Constraints",
			"App\\Services\\Redis\\": "src/App/RedisServices",
			"AuthBucket\\OAuth2\\": "authbucket/oauth2-php"
		}
	}
}
